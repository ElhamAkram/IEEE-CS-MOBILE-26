bool tidyNumber(int number) {
  String numStr = number.toString();
  for (int i = 0; i < numStr.length - 1; i++) {
    if (numStr[i].compareTo(numStr[i + 1]) > 0) {
      return false;
    }
  }
  return true;
}
void main() {
  print(tidyNumber(1234));    print(tidyNumber(112233));  print(tidyNumber(132));
}
